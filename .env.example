# OpenAI API Configuration / OpenAI API 配置
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Serper API for web search / 可选：用于网络搜索的 Serper API
# If not provided, the system will use simulated search results
# 如果未提供，系统将使用模拟搜索结果
SERPER_API_KEY=your_serper_api_key_here

# Logging Level / 日志级别
LOG_LEVEL=INFO
